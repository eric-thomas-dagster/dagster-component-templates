{
  "type": "dagster_component_templates.DatabaseQueryComponent",
  "name": "Database Query",
  "category": "extraction",
  "description": "Execute SQL queries against databases and return results as DataFrames",
  "icon": "Database",
  "x-dagster-io": {
    "outputs": {
      "type": "dataframe",
      "description": "Always produces a DataFrame from SQL query results"
    },
    "supports_partitions": true,
    "supported_partition_types": [
      "daily",
      "weekly",
      "monthly",
      "hourly"
    ]
  },
  "attributes": {
    "asset_name": {
      "type": "string",
      "label": "Asset Name",
      "description": "Name of the asset",
      "default": "query_result",
      "required": true
    },
    "database_url": {
      "type": "string",
      "label": "Database URL",
      "description": "Database connection URL",
      "default": "${DATABASE_URL}",
      "required": true
    },
    "query": {
      "type": "string",
      "label": "SQL Query",
      "description": "SQL query to execute",
      "default": "SELECT * FROM table_name",
      "required": true
    },
    "cache_to_parquet": {
      "type": "boolean",
      "label": "Cache to Parquet",
      "description": "Whether to cache results to parquet file",
      "default": false,
      "required": false
    },
    "cache_path": {
      "type": "string",
      "label": "Cache Path",
      "description": "Path to parquet cache file",
      "required": false
    },
    "description": {
      "type": "string",
      "label": "Description",
      "description": "Asset description",
      "required": false
    },
    "group_name": {
      "type": "string",
      "label": "Group Name",
      "description": "Asset group for organization",
      "required": false
    }
  },
  "tags": [
    "extraction",
    "ingestion",
    "data-source"
  ]
}