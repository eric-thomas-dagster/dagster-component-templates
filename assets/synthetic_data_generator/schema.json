{
  "type": "dagster_component_templates.SyntheticDataGeneratorComponent",
  "name": "Synthetic Data Generator",
  "category": "transformation",
  "description": "Generate realistic synthetic data for testing and development",
  "icon": "Wand",
  "x-dagster-io": {
    "outputs": {
      "type": "dataframe",
      "description": "Generated synthetic data as DataFrame"
    }
  },
  "attributes": {
    "asset_name": {
      "type": "string",
      "label": "Asset Name",
      "description": "Name of the asset",
      "required": true
    },
    "schema_type": {
      "type": "string",
      "label": "Schema Type",
      "description": "Type of data schema to generate",
      "default": "customers",
      "enum": [
        "customers",
        "orders",
        "products",
        "transactions",
        "events",
        "sensors",
        "users"
      ],
      "required": true
    },
    "row_count": {
      "type": "integer",
      "label": "Row Count",
      "description": "Number of rows to generate",
      "default": 100,
      "minimum": 1,
      "maximum": 100000,
      "required": true
    },
    "random_seed": {
      "type": [
        "integer",
        "null"
      ],
      "label": "Random Seed",
      "description": "Random seed for reproducible data generation (leave empty for random)",
      "default": null,
      "required": false
    },
    "description": {
      "type": "string",
      "label": "Description",
      "description": "Description of the asset",
      "default": "",
      "required": false
    },
    "group_name": {
      "type": "string",
      "label": "Group Name",
      "description": "Asset group name for organization",
      "default": "",
      "required": false
    }
  },
  "tags": [
    "transformation",
    "data-processing"
  ]
}