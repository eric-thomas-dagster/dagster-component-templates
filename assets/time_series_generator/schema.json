{
  "type": "dagster_component_templates.TimeSeriesGeneratorComponent",
  "name": "Time Series Generator",
  "category": "transformation",
  "description": "Generate time series data with trends, seasonality, and noise",
  "icon": "TrendingUp",
  "x-dagster-io": {
    "supports_partitions": true,
    "supported_partition_types": [
      "daily",
      "weekly",
      "monthly",
      "hourly"
    ]
  },
  "attributes": {
    "asset_name": {
      "type": "string",
      "label": "Asset Name",
      "description": "Name of the asset",
      "required": true
    },
    "pattern_type": {
      "type": "string",
      "label": "Pattern Type",
      "description": "Pattern to generate in the time series",
      "default": "complex",
      "enum": [
        "trend",
        "seasonal",
        "random_walk",
        "sine_wave",
        "step_function",
        "spike",
        "complex"
      ],
      "required": true
    },
    "start_date": {
      "type": "string",
      "label": "Start Date",
      "description": "Start date (YYYY-MM-DD format)",
      "default": "",
      "required": false
    },
    "end_date": {
      "type": "string",
      "label": "End Date",
      "description": "End date (YYYY-MM-DD format)",
      "default": "",
      "required": false
    },
    "frequency": {
      "type": "string",
      "label": "Frequency",
      "description": "Data point frequency",
      "default": "1h",
      "enum": [
        "1min",
        "5min",
        "15min",
        "30min",
        "1h",
        "1d"
      ],
      "required": false
    },
    "base_value": {
      "type": "number",
      "label": "Base Value",
      "description": "Starting/baseline value for the series",
      "default": 100.0,
      "required": false
    },
    "noise_level": {
      "type": "number",
      "label": "Noise Level",
      "description": "Amount of random noise to add (0.0 = none, 1.0 = high)",
      "default": 0.1,
      "minimum": 0.0,
      "maximum": 1.0,
      "required": false
    },
    "random_seed": {
      "type": [
        "integer",
        "null"
      ],
      "label": "Random Seed",
      "description": "Random seed for reproducible data generation (leave empty for random)",
      "default": null,
      "required": false
    },
    "metric_name": {
      "type": "string",
      "label": "Metric Name",
      "description": "Name of the metric/value column",
      "default": "value",
      "required": false
    },
    "description": {
      "type": "string",
      "label": "Description",
      "description": "Description of the asset",
      "default": "",
      "required": false
    },
    "group_name": {
      "type": "string",
      "label": "Group Name",
      "description": "Asset group name for organization",
      "default": "",
      "required": false
    }
  },
  "tags": [
    "transformation",
    "data-processing"
  ]
}