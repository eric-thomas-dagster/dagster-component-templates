{
  "type": "dagster_component_templates.VectorStoreQueryComponent",
  "name": "Vector Store Query",
  "category": "ai",
  "description": "Query vector stores for semantic search and similarity matching",
  "icon": "Search",
  "x-dagster-io": {
    "inputs": {
      "type": "array",
      "required": false,
      "description": "Accepts query embeddings from upstream asset via IO manager. Compatible types: NumPy array or Python list of embedding values. Optional if query_text parameter is provided (will generate embeddings internally)."
    },
    "outputs": {
      "type": "dataframe",
      "description": "Returns DataFrame with search results containing columns: 'id' (document identifier), 'score' (similarity score), 'text' (document content), and any additional metadata columns from the vector store"
    }
  },
  "attributes": {
    "asset_name": {
      "type": "string",
      "default": "search_results",
      "required": true
    },
    "provider": {
      "type": "string",
      "default": "chromadb",
      "enum": [
        "pinecone",
        "chromadb",
        "faiss",
        "qdrant"
      ],
      "required": true
    },
    "collection_name": {
      "type": "string",
      "default": "documents",
      "required": true
    },
    "query_text": {
      "type": "string",
      "description": "Direct query text (alternative to embedding)",
      "required": false
    },
    "top_k": {
      "type": "integer",
      "default": 10,
      "required": false
    },
    "connection_string": {
      "type": "string",
      "required": false
    },
    "api_key": {
      "type": "string",
      "description": "API key (use ${VAR_NAME} for environment variable)",
      "required": false
    },
    "include_distances": {
      "type": "boolean",
      "default": true,
      "required": false
    },
    "description": {
      "type": "string",
      "required": false
    },
    "group_name": {
      "type": "string",
      "required": false
    }
  },
  "tags": [
    "ai",
    "llm",
    "nlp"
  ]
}